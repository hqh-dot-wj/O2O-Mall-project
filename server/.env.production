# ========================================
# 纯环境变量配置示例（复制为 .env 后按需修改）
# ========================================

# 运行环境：development | test | production
NODE_ENV=production

# PostgreSQL 连接（Prisma 与运行时统一使用）
DATABASE_URL="postgresql://postgres:Llq123456@127.0.0.1:5432/nest_admin?schema=public"

# 应用配置
APP_PORT=8080
APP_PREFIX=/api

# 日志配置
LOG_LEVEL=info
LOG_PRETTY_PRINT=false
LOG_DIR=/var/log/nest-admin-soybean
LOG_EXCLUDE_PATHS=["/health","/metrics","/favicon.ico","/api-docs"]
LOG_SENSITIVE_FIELDS=["password","passwd","pwd","token","accessToken","refreshToken","access_token","refresh_token","authorization","cookie","secret","secretKey","apiKey","api_key"]

# 数据库细项（可选，覆盖配置对象）
DB_HOST=127.0.0.1
DB_PORT=5432
DB_USERNAME=postgres
DB_PASSWORD=Llq123456
DB_DATABASE=nest_admin
DB_SCHEMA=public
DB_SSL=false

# Redis 配置
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=123456
REDIS_DB=2
# Redis key 前缀 - 留空避免验证码key不匹配问题
# REDIS_KEY_PREFIX=
# REDIS_KEY_PREFIX=

# JWT 配置
JWT_SECRET=change_me_in_production
JWT_EXPIRES_IN=1h
JWT_REFRESH_EXPIRES_IN=2h

# 文件上传
FILE_IS_LOCAL=true
FILE_UPLOAD_LOCATION=../upload
FILE_MAX_SIZE=10
FILE_DOMAIN=http://localhost:8080
FILE_SERVE_ROOT=/profile

# 腾讯云 COS（可选）
# COS_SECRET_ID=
# COS_SECRET_KEY=
# COS_BUCKET=
# COS_REGION=
# COS_DOMAIN=
# COS_LOCATION=

# 白名单（可选，JSON 字符串）
# PERM_WHITELIST=[{"path":"/captchaImage","method":"GET"}]

# 代码生成器（可选）
# GEN_AUTHOR=linlingqin77
# GEN_PACKAGE_NAME=system
# GEN_MODULE_NAME=system
# GEN_AUTO_REMOVE_PRE=false
# GEN_TABLE_PREFIX=sys_

# 用户配置
USER_INITIAL_PASSWORD=123456

# 多租户配置 (已禁用)
# 设置为 true 可启用多租户功能
TENANT_ENABLED=true
TENANT_SUPER_ID=000000
TENANT_DEFAULT_ID=000000

